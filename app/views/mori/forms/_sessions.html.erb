<%= form_for @user, :url => sessions_path, :method => :post, :html => { :id => "new_session"} do |f| %>
  <h3>Log In</h3>
  <div class="form-group">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div class="form-group">
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <%= f.submit "Log In" %>
<% end %>
